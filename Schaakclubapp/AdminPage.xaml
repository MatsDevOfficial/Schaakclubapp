<Window x:Class="Schaakclubapp.AdminPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Admin - Evenementen Beheer" Height="400" Width="600">
    <Grid>
        <StackPanel>
            <TextBox Name="EventNameTextBox" Width="200" Margin="10" PlaceholderText="Evenement Naam"/>
            <TextBox Name="EventDateTextBox" Width="200" Margin="10" PlaceholderText="Evenement Datum"/>
            <Button Content="Voeg Evenement Toe" Click="AddEventButton_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
